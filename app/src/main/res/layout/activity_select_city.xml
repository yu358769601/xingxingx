<?xml version="1.0" encoding="utf-8"?>
<com.xinxinxuedai.view.initAction_Bar
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout_title"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home"
    tools:context="com.xinxinxuedai.ui.SelectCityActivity">
    <ScrollView
        android:layout_below="@+id/relativeLayout_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/selectcity_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RadioGroup
                android:id="@+id/selectcity_rg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </RadioGroup>
            <RadioGroup
                android:id="@+id/selectcity_rg2"
                android:layout_toRightOf="@+id/selectcity_rg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </RadioGroup>


        </RelativeLayout>
    </ScrollView>

</com.xinxinxuedai.view.initAction_Bar>
