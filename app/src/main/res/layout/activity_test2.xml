<?xml version="1.0" encoding="utf-8"?>
<com.xinxinxuedai.view.initAction_Bar
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout_title"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.xinxinxuedai.ui.Test2Activity">
    <Button
        android:id="@+id/test2_bt1"
        android:text="访问"
        android:layout_below="@+id/relativeLayout_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/test2_tv_updata"
        android:layout_below="@+id/test2_bt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/test2_tv"
        android:text="结果在这里显示"
        android:layout_below="@+id/test2_tv_updata"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.xinxinxuedai.view.initAction_Bar>
